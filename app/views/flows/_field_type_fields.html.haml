.panel.panel-default.nested-fields
  .panel-heading
    %h3.panel-title= f.object.class.name
  .panel-body
    = f.hidden_field :_type, value: f.object.class.name
    = f.hidden_field :index, value: f.index
    = render partial: "/flows/#{f.object.class.name.underscore}", object: f

    %p
      = link_to_remove_association "Remove field", f